<LinearLayout
	style="?attr/appBackground"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">

	<TextView
		android:id="@+id/caption"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="5dp"
		android:gravity="center"
		android:text="@string/personal_document_caption"
		android:textColor="@color/black"
		android:textSize="18sp"/>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton1"
			style="?attr/formButton"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:layout_weight="1"
			android:ellipsize="end"
			android:maxLines="1"/>

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton2"
			style="?attr/formButton"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:layout_weight="1"
			android:ellipsize="end"
			android:maxLines="1"/>

	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton3"
			style="?attr/formButton"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:layout_weight="1"
			android:ellipsize="end"
			android:maxLines="1"/>

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton4"
			style="?attr/formButton"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:layout_weight="1"
			android:ellipsize="end"
			android:maxLines="1"/>

	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton6"
			style="?attr/formButton"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:layout_weight="1"
			android:ellipsize="end"
			android:maxLines="1"/>

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton7"
			style="?attr/formButton"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_margin="5dp"
			android:layout_weight="1"
			android:ellipsize="end"
			android:maxLines="1"/>

	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">

		<ru.facilicom24.manager.views.FontButton
			android:id="@+id/photoFontButton8"
